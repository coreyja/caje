    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'slow_server'",
      "cargo": {
        "args": [
          "build",
          "--bin=slow_server",
          "--package=slow_server"
        ],
        "filter": {
          "name": "slow_server",
          "kind": "bin"
        }
      },
      "args": [],
